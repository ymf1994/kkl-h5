.custom-cascade {
  display flex
  flex-wrap wrap
  align-items center
  .select-component {
    width 10em!important
    overflow hidden
    margin-right 15px
    margin-bottom 15px
    .el-input {
      width 100%!important
    }
  }
  .add-subtract-btn {
    width 40px
    height 40px
    display flex
    align-items center
    justify-content center
    cursor pointer
    margin-bottom 20px
  }
  
  .el-select, .el-input {
    width 220px!important
  }
}
